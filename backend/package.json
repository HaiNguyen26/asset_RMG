{
  "name": "asset-management-api",
  "version": "1.0.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:create": "node scripts/create-database.js",
    "db:setup": "npm run db:create && npm run prisma:migrate && npm run prisma:seed",
    "fix": "npm install && npm run prisma:generate",
    "fix-all": "npm install && npm run prisma:generate && npm run prisma:migrate:auto && npm run prisma:seed",
    "create-admin": "ts-node scripts/create-admin.ts",
    "clear-mock-assets": "ts-node scripts/clear-mock-assets.ts",
    "clear-mock-departments": "ts-node scripts/clear-mock-departments.ts",
    "clear-all-data": "ts-node scripts/clear-all-data.ts",
    "clear-all-keep-it": "ts-node scripts/clear-all-keep-it.ts",
    "add-equipment-policy": "ts-node scripts/add-equipment-assignment-policy.ts",
    "check-user": "ts-node scripts/check-user.ts",
    "test-connection": "ts-node scripts/test-connection.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:auto": "prisma migrate dev --name add_users_and_employees_code",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@prisma/client": "^7.3.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/multer": "^1.4.12",
    "@types/bcrypt": "^5.0.2",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1",
    "prisma": "^7.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
